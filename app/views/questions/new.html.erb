<h1>Create New Question</h1>

<form action="/tests/<%= params[:test_id] %>/questions" method="post" accept-charset="utf-8">
  <input type="hidden" name="authenticity_token" id="authenticity_token" value="<%= form_authenticity_token %>" />
  <p>
    <label for="question_body">Body:</label>
    <input type="text" name="question[body]" id="question_body" />
  </p>
    <input type="submit" name="submit" id="submit" value="create" />
</form>

